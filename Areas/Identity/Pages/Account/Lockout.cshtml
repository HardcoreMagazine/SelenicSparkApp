@page
@model LockoutModel
@{
    ViewData["Title"] = "Account Locked";
}

<header style="text-align:center; padding-top:14%;">
    <h1 class="text-danger"><b>@ViewData["Title"]</b></h1>
    <p class="text-danger" style="padding-top:10px;">
        <b>This account has been temporarily banned</b>.
        <br />
        Contact site administrator at <a href="mailto:admin@selenicspark.org">admin@selenicspark.org</a> if you think it was a mistake.
    </p>
</header>
